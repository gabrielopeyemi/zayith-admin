{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/webpack",
        "config": {
          "webpack": {
            "output": {
              "path": "/out"
            }
          }
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  